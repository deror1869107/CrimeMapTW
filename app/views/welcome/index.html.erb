

<div class="container">

    <div class="row">
        <div class="box">
            <div class="col-lg-12 text-center">

				<div class="map" style="margin: auto;"></div>

            </div>
        </div>
    </div>

</div>
<!-- /.container -->


<script>
// Javascript

$('.map').tinyMap({
    'center': '台北市信義區台北101',
    'zoom'  : 14,
     'marker': [
         <% i = 0 %>
        <% Map.last.data.each do |data| %>
            {
            'addr': ['<%= data.lat %>', '<%= data.lng %>'],
            'text': '<strong><%= data.label %></strong>',
            'animation': 'DROP'
        },
        <% end %>
    ]
    });

</script>
